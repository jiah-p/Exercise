# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None
#
# 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
#
# 
# @param preOrder int整型一维数组 
# @param vinOrder int整型一维数组 
# @return TreeNode类
#
class Solution:
    def buildTree(self, preorder, inorder):
        if not preorder or not inorder:
            return None
        root = TreeNode(preorder[0])
        in_root = inorder.index(root.val) #3

        root.left = self.buildTree(preorder[1:in_root+1], inorder[:in_root])
        root.right = self.buildTree(preorder[in_root+1:], inorder[in_root+1:])

        return root

    def reConstructBinaryTree(self , preOrder: List[int], vinOrder: List[int]) -> TreeNode:
        # write code here
        return self.buildTree(preOrder,vinOrder)
        
