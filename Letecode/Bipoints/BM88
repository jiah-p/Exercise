class Solution:
    def judge(self , str: str) -> bool:
        # write code here
        # if not str:
        #     return True
        # i = 0
        # j = len(str) -1
        # while i < j:
        #     if str[i] != str[j]:
        #         return False
        #     i+=1
        #     j-=1
        # return True
        if str == str[::-1]:
            return True
        return False
        