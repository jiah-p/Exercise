#
# 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
#
# 反转字符串
# @param str string字符串 
# @return string字符串
#
class Solution:
    def solve(self , str: str) -> str:
        # write code here
        # return str[::-1]
        ans = ''
        for i in range(len(str)-1,-1,-1):
            ans += str[i]

        return ans